  "39": {"a":{"g":"Or has He daughters while you have sons?"},"w":[{"b":"0.04","c":"أَمۡ","d":"am","e":"Or","h":"0.55"},{"b":"0.4","c":"لَهُ","d":"lahu","e":"for Him","h":"0.98"},{"b":"0.87","c":"ٱلۡبَنَٰتُ","d":"l-banātu","e":"(are) daughters","h":"1.51"},{"b":"2.0","c":"وَلَكُمُ","d":"walakumu","e":"while for you","h":"3.05"},{"b":"2.9","c":"ٱلۡبَنُونَ","d":"l-banūna","e":"(are) sons","h":"4.24"}]},
