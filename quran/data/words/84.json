  "4": {"a":{"g":"And has cast out that within it and relinquished [it]"},"w":[{"b":"0.03","c":"وَأَلۡقَتۡ","d":"wa-alqat","e":"And has cast out","h":"0.25"},{"b":"1.0","c":"مَا","d":"mā","e":"what","h":"1.8"},{"b":"1.3","c":"فِيهَا","d":"fīhā","e":"(is) in it","h":"2.56"},{"b":"2.2","c":"وَتَخَلَّتۡ","d":"watakhallat","e":"and becomes empty","h":"4.24"}]},
